{
  "name": "nativets",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prettier-code": "prettier --write src/**/*.{ts,tsx} --config ./.prettierrc",
    "lint": "eslint \"**\\\\*.{ts,tsx}\" --quiet --color --cache --report-unused-disable-directives --fix",
    "format": "npm run lint && npm run prettier-code",
    "reinstall": "npm install",
    "remove-modules": "npx rimraf node_modules",
    "metro-start": "expo start",
    "metro-start:clear": "expo start --reset-cache",
    "doctor": "npx expo-doctor",
    "build:web": "npx serve dist && npx expo export -p web",
    "build:android": "npm i eas-cli && eas build -p android --profile preview"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "eas-cli": "^5.7.0",
    "expo": "~49.0.15",
    "expo-blur": "~12.4.1",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-drawer-layout": "^3.2.2",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "typescript": "^5.1.3"
  },
  "private": true,
  "resolutions": {
    "react-native-reanimated": "3.3.0"
  }
}
